<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #646E78;"
            xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="giba.controller.EditProfileController">
    <AnchorPane layoutY="100.0" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;">
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="300.0" layoutY="14.0"
                   pickOnBounds="true" preserveRatio="true">
            <Image url="@../resources/user_100px.png"/>
        </ImageView>
        <GridPane layoutX="212.0" layoutY="195.0" prefHeight="150.0" prefWidth="377.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="377.0" minWidth="10.0" prefWidth="377.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="0.0" prefWidth="0.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <JFXTextField fx:id="firstName" prefWidth="330.0" GridPane.rowIndex="1"/>
            <JFXTextField fx:id="lastName" GridPane.rowIndex="2"/>
            <JFXTextField fx:id="username" GridPane.rowIndex="3"/>
            <JFXTextField fx:id="password" GridPane.rowIndex="4"/>
        </GridPane>
        <JFXButton layoutX="343.0" layoutY="383.0" onAction="#saveProfile" prefHeight="40.0" prefWidth="115.0"
                   style="-fx-background-color: #8D98A7;" text="Save" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </JFXButton>
        <Label layoutX="131.0" layoutY="215.0" text="First Name">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <Label layoutX="131.0" layoutY="250.0" text="Last Name">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <Label layoutX="131.0" layoutY="290.0" text="Username">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <Label layoutX="131.0" layoutY="328.0" text="Password">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
    </AnchorPane>
    <JFXButton layoutX="14.0" layoutY="41.0" onAction="#back" prefHeight="30.0" prefWidth="80.0"
               style="-fx-background-color: #8D98A7;" text="Back" textFill="WHITE">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </JFXButton>
    <Label layoutX="337.0" layoutY="39.0" text="Edit Profile" textFill="WHITE">
        <font>
            <Font name="System Bold" size="24.0"/>
        </font>
    </Label>
</AnchorPane>
